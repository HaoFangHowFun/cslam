<launch>
   <!-- Robot 0 -->
   <include file="$(find-pkg-share cslam)/launch/sensors/bag_kitti.xml">
      <arg name="bag_start_delay" value="10.0"/>
      <arg name="namespace" value="/r0"/>
      <arg name="sequence" value="0"/>
   </include>
   <include file="$(find-pkg-share cslam)/launch/cslam.launch.py">
      <arg name="namespace" value="/r0"/>
      <arg name="robot_id" value="0"/>
      <arg name="nb_robots" value="2"/>
   </include>
   <!-- Robot 1 -->
   <include file="$(find-pkg-share cslam)/launch/sensors/bag_kitti.xml">
      <arg name="bag_start_delay" value="10.0"/>
      <arg name="namespace" value="/r1"/>
      <arg name="sequence" value="1"/>
   </include>
   <include file="$(find-pkg-share cslam)/launch/cslam.launch.py">
      <arg name="namespace" value="/r1"/>
      <arg name="robot_id" value="1"/>
      <arg name="nb_robots" value="2"/>
   </include>
</launch>